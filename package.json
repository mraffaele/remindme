{
  "name": "remindme",
  "version": "1.0.1",
  "description": "A simple desktop application allowing you to create and schedule re-occurring personal reminders",
  "author": "Michael Raffaele <michael@mraffaele.com> (http://github.com/mraffaele)",
  "homepage": "https://github.com/mraffaele/remindme",
  "repository": "https://github.com/mraffaele/remindme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mraffaele/remindme/issues"
  },
  "main": "main.js",
  "scripts": {
    "dev": "webpack -w --mode=development",
    "prod": "webpack -p --mode=production",
    "package-win": "npm run prod && electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=./src/assets/icon/icon.ico --appname=RemindMe",
    "package-mac": "npm run prod && electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --appname=RemindMe"
  },
  "keywords": [
    "electron",
    "electronjs",
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "typescript",
    "sass",
    "reminders",
    "calendar",
    "scheduling",
    "time"
  ],
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^10.5.0",
    "@types/react": "^16.4.4",
    "@types/react-redux": "^6.0.3",
    "@types/redux": "^3.6.0",
    "cron": "^1.3.0",
    "css-loader": "^0.28.11",
    "dataurl": "^0.1.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.3",
    "electron-connect": "^0.6.3",
    "electron-packager": "^12.1.0",
    "es6-promise-promise": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-normalize-scss": "^8.0.0",
    "node-sass": "^4.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "uniqid": "^5.0.3",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-electron-connect-plugin": "^1.0.4"
  },
  "dependencies": {}
}
